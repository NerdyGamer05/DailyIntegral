{
  "routes": [
    {
      "src": "^/profile/(.*)",
      "dest": "/profile/$1",
      "status": 200
    },
    {
      "src": "^/problem/(.*)",
      "dest": "/problem/$1",
      "status": 200
    },
    {
      "src": "^/leaderboard",
      "dest": "/leaderboard",
      "status": 200
    },
    {
      "src": "^/callback",
      "dest": "/callback",
      "status": 200
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "status": 200
    }
  ]
}